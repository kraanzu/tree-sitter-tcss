{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "tcss",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "rule_set"
          },
          {
            "type": "SYMBOL",
            "name": "variable_declaration"
          }
        ]
      }
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "# "
            },
            {
              "type": "PATTERN",
              "value": "[^\\n]*"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "/*"
            },
            {
              "type": "PATTERN",
              "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
            },
            {
              "type": "STRING",
              "value": "/"
            }
          ]
        }
      ]
    },
    "variable_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "value_list"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "rule_set": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "selector_group"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "rule_set"
              },
              {
                "type": "SYMBOL",
                "name": "declaration"
              },
              {
                "type": "SYMBOL",
                "name": "comment"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "property_name"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "value_list"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "selector_group": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "selector"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "selector"
              }
            ]
          }
        }
      ]
    },
    "selector": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "simple_selector"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "combinator"
              },
              {
                "type": "SYMBOL",
                "name": "simple_selector"
              }
            ]
          }
        }
      ]
    },
    "combinator": {
      "type": "STRING",
      "value": ">"
    },
    "simple_selector": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "type_selector"
            },
            {
              "type": "SYMBOL",
              "name": "id_selector"
            },
            {
              "type": "SYMBOL",
              "name": "wildcard_selector"
            },
            {
              "type": "SYMBOL",
              "name": "parent_reference"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "class_selector"
              },
              {
                "type": "SYMBOL",
                "name": "pseudo_class_selector"
              }
            ]
          }
        }
      ]
    },
    "type_selector": {
      "type": "PATTERN",
      "value": "[A-Z_][a-zA-Z0-9_-]*"
    },
    "class_selector": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
        }
      ]
    },
    "id_selector": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
        }
      ]
    },
    "pseudo_class_selector": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "PATTERN",
          "value": "[a-z_][a-z_-]*"
        }
      ]
    },
    "wildcard_selector": {
      "type": "STRING",
      "value": "*"
    },
    "parent_reference": {
      "type": "STRING",
      "value": "&"
    },
    "property_name": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-z][a-z0-9_-]*"
      }
    },
    "value_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "value"
              },
              {
                "type": "SYMBOL",
                "name": "variable_reference"
              },
              {
                "type": "SYMBOL",
                "name": "function_call"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "important"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "important": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "?important"
      }
    },
    "value": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z0-9_%#\\.-]+"
      }
    },
    "function_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "rgb"
            },
            {
              "type": "STRING",
              "value": "rgba"
            },
            {
              "type": "STRING",
              "value": "hsl"
            },
            {
              "type": "STRING",
              "value": "hsla"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "value"
              },
              {
                "type": "STRING",
                "value": ","
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
        }
      ]
    },
    "variable_reference": {
      "type": "SYMBOL",
      "name": "variable"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s|\\\\\\r?\\n"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}